{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"types": ["bun-types"],
		"strictNullChecks": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@assets/*": ["src/assets/*"],
			"@content/*": ["src/content/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@data/*": ["src/data/*"],
		}
	}
}
